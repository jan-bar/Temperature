;;;;;;;;;;;;;;;;;;;;;;;;LCD12864
LCD_RS		BIT			P2.2
LCD_RW		BIT			P2.1
LCD_E		BIT			P2.0
LCD_CS1		BIT			P2.4	;低电平有效，左半屏
LCD_CS2		BIT			P2.3	;低电平有效，右半屏
;;;;;;;;;;;;;;;;;;;;;;;;DS1302
T_CLK 		BIT 		P1.3
T_IO		BIT 		P1.4
T_RST		BIT 		P1.2
SECOND		EQU 		30H
MINUTE		EQU 		31H
HOUR		EQU 		32H
DAY			EQU		 	33H
MONTH		EQU 		34H
WEEK		EQU 		35H
YEAR		EQU 		36H
;;;;;;;;;;;;;;;;;;;;;;;;24C64
SCL			BIT			P1.0
SDA			BIT			P1.1
SLA			EQU			37H
SUBA		EQU			38H
MTD			EQU			39H
MRD			EQU			3AH
;;;;;;;;;;;;;;;;;;;;;;;;18B20
TEMP_ZH		EQU			3BH			 ;实际温度值存放单元，用来和报警温度值比较
TEMPL		EQU			3CH			 ;温度值低位
TEMPH		EQU			3DH			 ;温度值高位
TEMP_TH		EQU			3EH			 ;高温报警值，内部命令时用
TEMP_TL		EQU			3FH			 ;低温报警值，内部命令时用
FLAG		BIT			20H.0		 ;DS18B20是否存在标记
DQ			BIT			P1.5		 ;DS18B20数据脚定义
TEMPHC		EQU			40H			 ;温度转换低8位	
TEMPLC		EQU			41H			 ;温度转换高8位
CONUT_DS	EQU			42H			 ;定义温度采集时间
TEMPHH1		EQU			43H			 ;第一个温度计的高温预警
TEMPLL1		EQU			44H			 ;第一个温度计的低温预警
TEMPHH2		EQU			45H			 ;第二个温度计的高温预警
TEMPLL2		EQU			46H			 ;第二个温度计的低温预警
TEMP_LED	EQU			47H			 ;报警灯亮灭控制
MUSIC0		EQU			48H
MUSIC1		EQU			49H
CONUT_ALARM	EQU			50H
;;;;;;;;;;;;;;;;;;;;;;;;按键定义
SELECT		BIT			P2.5
OK			BIT			P2.6
KADD		BIT			P2.7
KDEC		BIT			P1.6
SPEALER		BIT			P1.7
KOK			BIT			P3.5
;;;;;;;;;;;;;;;;;;;;;;;;
	ORG 	0000H
	LJMP	MAIN0
	ORG		0003H
	LJMP	INT0P
	ORG		0013H
	LJMP	INT1P
	ORG 	000BH
	LJMP 	TIME1
	ORG		0030H
MAIN0:
;;;;;;;;;;;;;;;;;;;;;;;;
				MOV		SP,#50H		;堆栈初始化
				MOV		IE,#87H		;开中断，T0和INT0
				SETB	IT0			;设INT0下降沿触发
				SETB	IT1			;设INT1下降沿触发
				MOV 	TMOD,#01H
				MOV		SCON,#0		;串口工作在方式0
				MOV		PCON,#0
				;;;;;;;
				MOV		A,#80		;设高温80度报警，可更改
				MOV		TEMPHH1,A
				MOV		TEMPHH2,A
				MOV		A,#25		;设低温-25度报警，可更改,注意
				;;;;温度存储时没有带上符号位，比较大小时要注意
				MOV		TEMPLL1,A
				MOV		TEMPLL2,A
				;;;;;;
				;以R2来记录所亮的灯的信息
				MOV		TEMP_LED,#0
				;;;;;;;;起始为0，如果有需要亮的等就ORL那一位
				LCALL	DS1302_INIT	;初始化DS1302
				LCALL	LCD_MOD1	;显示MOD1方式LCD
				MOV		CONUT_ALARM,#1

SMAIN:			
				LCALL	DISP_1302_LCD;显示时间值到LCD
				LCALL	DS18B20_TEMP  ;显示温度值
				DJNZ	CONUT_ALARM,ALARM_OK
				LCALL	ALARM		  ;过温报警
				MOV		CONUT_ALARM,#1
ALARM_OK:		
				LJMP	SMAIN
;;;;;;;;;;;;;;;;;;;;;;;;温度报警程序
ALARM:			
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
				MOV		R7,#1
				LCALL	READ_TEMP
				LCALL	READ_TEMP	;读第一个温度的值
				MOV		A,TEMPHC
				ANL		A,#0F0H
				CJNE	A,#00H,ALARM_ZHENG1
				MOV		A,TEMP_ZH	;A接收实际温度值，十六进制的数
				SUBB	A,TEMPLL1
				JNB		ACC.7,LLALARM1	;低温警报
				LJMP	ANQUAN1			;安全状态
ALARM_ZHENG1:	;;;;;;;;;
				MOV		A,TEMP_ZH	;A接收实际温度值，十六进制的数
				SUBB	A,TEMPHH1
				JNB		ACC.7,HHALARM1	;高温警报
				LJMP	ANQUAN1			;安全状态
;;;;;;;;;;;;;;;;;;;
HHALARM1:		MOV		A,TEMP_LED
				ORL		A,#01H
				ANL		A,#0FDH		;保证亮高温的那一盏
				MOV		TEMP_LED,A
				MOV		SBUF,A		   ;传送当前亮的灯
HLOP1:			JBC		TI,HK1
				LJMP	HLOP1
HK1:			
				LJMP	ALARM_END1
;;;;;;;;;;;;;;;;;;
LLALARM1:	    MOV		A,TEMP_LED
				ORL		A,#02H
				ANL		A,#0FEH		;保证亮低温那一盏
				MOV		TEMP_LED,A
				MOV		SBUF,A		   ;传送当前亮的灯
LLOP1:			JBC		TI,LK1
				AJMP	LLOP1
LK1:
				LJMP	ALARM_END1
;;;;;;;;;;;;;;;;;;
ANQUAN1:		;;;;;
				MOV		A,TEMP_LED
				ANL		A,#0FCH		;1安全时灭它对应的灯
				MOV		TEMP_LED,A
				MOV		SBUF,A		   ;传送当前亮的灯
ALOP1:			JBC		TI,AK1
				LJMP	ALOP1
AK1:

ALARM_END1:
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
				MOV		R7,#2
				LCALL	READ_TEMP
				LCALL	READ_TEMP	;读第二个温度的值
				MOV		A,TEMPHC
				ANL		A,#0F0H
				CJNE	A,#00H,ALARM_ZHENG2
				MOV		A,TEMP_ZH	;A接收实际温度值，十六进制的数
				SUBB	A,TEMPLL2
				JNB		ACC.7,LLALARM2	;低温警报
				LJMP	ANQUAN2			;安全状态
ALARM_ZHENG2:	;;;;;;;;;
				MOV		A,TEMP_ZH	;A接收实际温度值，十六进制的数
				SUBB	A,TEMPHH2
				JNB		ACC.7,HHALARM2	;高温警报
				LJMP	ANQUAN2			;安全状态
;;;;;;;;;;;;;;;;;;;
HHALARM2:		MOV		A,TEMP_LED
				ORL		A,#04H
				ANL		A,#0F7H		;保证亮高温的那一盏
				MOV		TEMP_LED,A
				MOV		SBUF,A		   ;传送当前亮的灯
HLOP2:			JBC		TI,HK2
				LJMP	HLOP2
HK2:
				LJMP	ALARM_END
;;;;;;;;;;;;;;;;;;
LLALARM2:	    MOV		A,TEMP_LED
				ORL		A,#08H
				ANL		A,#0FBH		;保证亮低温那一盏
				MOV		TEMP_LED,A
				MOV		SBUF,A		   ;传送当前亮的灯
LLOP2:			JBC		TI,LK2
				LJMP	LLOP2
LK2:
				LJMP	ALARM_END
;;;;;;;;;;;;;;;;;;
ANQUAN2:		;;;;;
				MOV		A,TEMP_LED
				ANL		A,#0F3H		;1安全时灭它对应的灯
				MOV		TEMP_LED,A
				MOV		SBUF,A		   ;传送当前亮的灯
ALOP2:			JBC		TI,AK2
				LJMP	ALOP2
AK2:
	 			
ALARM_END:		
				MOV		A,TEMP_LED
				ANL		A,#0FH
				JZ		QANQUAN			;全部都安全
				MOV		A,TEMP_LED
				JNB		PSW.0,HLWEN		;有偶数个1，则低温高温都有
				MOV		A,TEMP_LED
				ANL		A,#05H
				JZ		DIWEN				;A等于零则是低温
				MOV		DPTR,#TAB_GAO		;高温表
				MOV		MUSIC0,DPL
				MOV		MUSIC1,DPH
				MOV		DPTR,#TAB_HOT
				LCALL	DISPLAY_BMP
				AJMP	WEN_END
DIWEN:			MOV		DPTR,#TAB_DI		;低温表
				MOV		MUSIC0,DPL
				MOV		MUSIC1,DPH
				MOV		DPTR,#TAB_COLD
				LCALL	DISPLAY_BMP
				AJMP	WEN_END
HLWEN:			MOV		DPTR,#TAB_GAO_DI	;高低温都有
				MOV		MUSIC0,DPL
				MOV		MUSIC1,DPH
				MOV		DPTR,#TAB_H_C
				LCALL	DISPLAY_BMP
WEN_END:
				
				POP		DPH
				POP		DPL
				MOV		DPTR,#ALARM_MUSIC
				PUSH	DPL
				PUSH	DPH

QANQUAN:				
				RET
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;音乐段程序
ALARM_MUSIC: 
  			   	MOV		DPH,MUSIC1
				MOV		DPL,MUSIC0
LOOP:			
				
				CLR A
				MOVC A,@A+DPTR
				MOV R1,A
				INC DPTR
				CLR A
				MOVC A,@A+DPTR
				MOV R0,A
				ORL A,R1
				JZ XZF;;;;;;
				MOV A,R0
				ANL A,R1
				CJNE A,#0FFH,NEXT
				AJMP ALARM_MUSIC
	;;;;
NEXT:
				MOV TH0,R1
				MOV TL0,R0
				SETB TR0
				SJMP NEXT1
	;;;
XZF:
				CLR TR0
NEXT1:
				CLR A
				INC DPTR
				MOVC A,@A+DPTR
				MOV R2,A
				SETB TR0
LOOP1:
				LCALL D130
				DJNZ R2,LOOP1
				INC DPTR
				AJMP LOOP

