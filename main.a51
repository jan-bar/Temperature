				JNB		OK,$
				LJMP	SELECT_1820_2_H
OK4:			JB		SELECT,SELECT4
				JNB		SELECT,$
				MOV		R5,#2
				MOV		R6,#6
				MOV		R7,#3
				MOV		DPTR,#TAB0_9+240
				LCALL	DISPLAY_LCD
				MOV		R5,#2
