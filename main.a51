D130:
	MOV R7,#120
DEL0:
	MOV R6,#200
DEL1:
	NOP
	NOP
	DJNZ R6,DEL1
	DJNZ R7,DEL0
	RET
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;; 
;;;;;;;;;;;;;;;;;;;;;;;; 按键中断									 
INT0P:			;按任意键一下进入中断								 
				;;;	k1为选择键，K2为确认键，K3为加键，K4为减键		 
				;;;;;;;
				CLR		EA		;进入后关中断，完成整套操作才退出
				MOV		A,#0
				LCALL	CLEAR_SCREEN
				LCALL	LCD_MOD2
				;;;;;
				;选择哪个温度计
				;;;;;
PANDUAN0:		
				JNB		SELECT,$
				MOV		R5,#1
				MOV		R6,#2
				MOV		R7,#36
				MOV		DPTR,#TAB_ZF+72
				LCALL	DISPLAY_LCD
				MOV		R5,#1
				MOV		R6,#4
				MOV		R7,#36
				MOV		DPTR,#TAB0_9+240
				LCALL	DISPLAY_LCD
SELECT0:		JB		OK,OK0
				JNB		OK,$
				LJMP	SELECT_1820_1
OK0:			JB		SELECT,SELECT01
				JNB		SELECT,PANXIA	  ;判断下一个
SELECT01:		
				JB		KOK,SELECT0
				JNB		KOK,$
				LJMP	INTOP_END

PANXIA:			JNB		SELECT,$
				MOV		R5,#1
				MOV		R6,#4
				MOV		R7,#36
				MOV		DPTR,#TAB_ZF+72
				LCALL	DISPLAY_LCD
				MOV		R5,#1
				MOV		R6,#2
				MOV		R7,#36
				MOV		DPTR,#TAB0_9+240
				LCALL	DISPLAY_LCD
SELECT22:		JB		OK,OK1
				JNB		OK,$
				LJMP	SELECT_1820_2
OK1:			JB		SELECT,SELECT1
				JNB		SELECT,$
				LJMP	PANDUAN0

SELECT1:		JB		KOK,SELECT22
				JNB		KOK,$
				LJMP	INTOP_END
				
SELECT_1820_1:	
				JNB		SELECT,$
				MOV		R5,#2
				MOV		R6,#0
				MOV		R7,#3
				MOV		DPTR,#TAB_ZF+96
				LCALL	DISPLAY_LCD
				MOV		R5,#2
				MOV		R6,#0
				MOV		R7,#43
				MOV		DPTR,#TAB0_9+240
				LCALL	DISPLAY_LCD
SELECT2:		JB		OK,OK2
				JNB		OK,$
				LJMP	SELECT_1820_1_H
OK2:			JB		SELECT,SELECT2
				JNB		SELECT,$
				MOV		R5,#2
				MOV		R6,#0
				MOV		R7,#3
				MOV		DPTR,#TAB0_9+240
				LCALL	DISPLAY_LCD
				MOV		R5,#2
				MOV		R6,#0
				MOV		R7,#43
				MOV		DPTR,#TAB_ZF+96
				LCALL	DISPLAY_LCD
SELECT3:		JB		OK,OK3
				JNB		OK,$
				LJMP	SELECT_1820_1_L
OK3:			JB		SELECT,SELECT3
				LJMP	SELECT_1820_1
SELECT_1820_1_H:;;;;对第一个温度计的高温部分操作
KDEC1_H:		JB		KADD,KADD1_H
				JNB		KADD,$
				INC		TEMPHH1		
				LCALL	LCD_MOD2
				AJMP	KDEC1_H
KADD1_H:		JB		KDEC,KOK1_H
				JNB		KDEC,$
				DEC		TEMPHH1
				LCALL	LCD_MOD2
				AJMP	KDEC1_H
KOK1_H:			JB		OK,KDEC1_H
				JNB		OK,$
				LJMP	SELECT_1820_1
SELECT_1820_1_L:;;;;对第一个温度计的低温部分操作
KDEC1_L:		JB		KADD,KADD1_L
				JNB		KADD,$
				INC		TEMPLL1		
				LCALL	LCD_MOD2
				AJMP	KDEC1_L
KADD1_L:		JB		KDEC,KOK1_L
				JNB		KDEC,$
				DEC		TEMPLL1
				LCALL	LCD_MOD2
				AJMP	KDEC1_L
KOK1_L:			JB		OK,KDEC1_L
				JNB		OK,$
				LJMP	PANDUAN0
