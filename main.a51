				LJMP	SELECT_1820_2_L
OK5:			JB		SELECT,SELECT5
				LJMP	SELECT_1820_2
SELECT_1820_2_H:;;;;对第二个温度计的高温部分操作
KDEC2_H:		JB		KADD,KADD2_H
				JNB		KADD,$
				INC		TEMPHH2		
				LCALL	LCD_MOD2
				AJMP	KDEC2_H
KADD2_H:		JB		KDEC,KOK2_H
				JNB		KDEC,$
				DEC		TEMPHH2
				LCALL	LCD_MOD2
				AJMP	KDEC2_H
KOK2_H:			JB		OK,KDEC2_H
				JNB		OK,$
				LJMP	SELECT_1820_2
SELECT_1820_2_L:;;;;对第二个温度计的低温部分操作
KDEC2_L:		JB		KADD,KADD2_L
				JNB		KADD,$
				INC		TEMPLL2		
				LCALL	LCD_MOD2
				AJMP	KDEC2_L
KADD2_L:		JB		KDEC,KOK2_L
				JNB		KDEC,$
				DEC		TEMPLL2
				LCALL	LCD_MOD2
				AJMP	KDEC2_L
KOK2_L:			JB		OK,KDEC2_L
				JNB		OK,$
				LJMP	PANDUAN0
